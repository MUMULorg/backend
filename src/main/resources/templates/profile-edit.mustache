<div class="container" id="main">
    <div class = "col-md-6 col-md-offset-3">
        {{#userName}}
            <form>
                    <label for="id"></label>
                    <div class="form-group">
                    <label for="username">닉네임</label>
                        <input type="text" id="username" class="form-control" value="{{userDTO.name}}"/>
                    </div>

                    <div class="form-group">
                        <label for="introduce">소개</label>
                        <input type="text" id="introduce" class="form-control" value="{{userDTO.introduce}}"/>
                    </div>

                    <div class="form-group">
                        <label for="picture">프로필</label>
                        <input type="text" id="picture" class="form-control" value="{{userDTO.picture}}"/>
                    </div>

                        <div class="form-group">
                        <label for="instaId">인스타 계정</label>
                        <input type="text" id="instaId" class="form-control"/>
                    </div>
                </form>
        {{/userName}}
    </div>
</div>